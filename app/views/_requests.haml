.table-header
  .table-header-title
    %h3 Time Waiting
  .table-header-title
    %h3 Customer Name
  .table-header-title
    %h3 Request Time
.table-content
  - customer_requests.each do |customer_request|
    .request-time-waiting
      %p= customer_request[:minutes_spent_waiting]
    .request-customer-name
      %p= customer_request[:name]
    .request-started-waiting
      %p= customer_request[:time_started_waiting]
